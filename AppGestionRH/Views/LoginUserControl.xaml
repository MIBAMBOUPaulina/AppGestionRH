<UserControl x:Class="AppGestionRH.Views.LoginUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Width="400" Height="300">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Width="300">
            <TextBlock Text="Identifiant :" Margin="10" />
            <TextBlock Text="Mot de passe :" Margin="10" />
            <PasswordBox x:Name="PasswordBox" Margin="10" />
            <Button Content="Connexion" Click="LoginButton_Click" Margin="10" />
        </StackPanel>
    </Grid>
</UserControl>



